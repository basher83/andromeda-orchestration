{
  "mcpServers": {
    "deepwiki": {
      "type": "http",
      "url": "https://mcp.deepwiki.com/mcp"
    },
    "grep": {
      "type": "http",
      "url": "https://mcp.grep.app"
    },
    "infisical": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "@infisical/mcp@latest"
      ],
      "env": {}
    },
    "firecrawl": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "firecrawl-mcp"
      ],
      "env": {
        "FIRECRAWL_API_KEY": "${FIRECRAWL_API_KEY}"
      }
    },
    "github": {
      "type": "stdio",
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-e",
        "GITHUB_PERSONAL_ACCESS_TOKEN",
        "-e",
        "GITHUB_TOOLSETS=repos,issues,pull_requests,actions,code_security",
        "ghcr.io/github/github-mcp-server"
      ],
      "env": {}
    },
    "coderabbitai": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "coderabbitai-mcp@latest"
      ],
      "env": {
        "GITHUB_PAT": "${GITHUB_PERSONAL_ACCESS_TOKEN}"
      }
    },
    "netdata": {
      "type": "stdio",
      "command": "/usr/local/netdata/usr/sbin/nd-mcp",
      "args": [
        "ws://192.168.30.50:19999/mcp?api_key=688656a9-ef64-4b54-a9de-b9860a39bd33"
      ]
    }
  }
}